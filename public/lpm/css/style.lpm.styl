/* ASIDE MENU */
.aside-fixed {
  top: 0;
  bottom: 0;
  border-width: 0 0 1px;
  width: 500px;
  right: -500px;
  background-color: #000000;
  position: fixed;
  z-index:9998;
  transition: right .5s ease-in-out;
}
.aside-fixed-fullwidth{
  width: 100%;
}
.aside-fixed.opened {
  right: 0;
}
.aside-header {
  padding: 30px 30px 20px 0;
}
.aside-fixed .nav>li>a, #subscribe {
  padding: 10px 15px 10px 30px;
}
.open-aside-cnt {
  display:table;
}
.open-aside-cnt {
  position: absolute;
  right: 10px;
  top: 10px;
}
a.open-aside-circle {
  position: relative;
  display: inherit;
  vertical-align: middle;
  text-align: center;
  height: 30px;
  width: 30px;
  background-color: #fff;
  color: #000;
  border-radius: 50%;
  font-size: 15px;
  padding: 2px 0 0 2px;
}
a.open-aside-circle:hover i{
  opacity: .6;
}
.open-aside-cell {
    position: relative;
    display: table-cell;
}
.aside-fixed .navbar-nav>li {
  float: none;
}
.aside-fixed .navbar-inverse {
  background: none;
}
.aside-fixed .navbar-nav>li {
  float: none;
}
.aside-fixed .navbar-nav {
  margin: 0;
}
.aside-fixed .navbar-nav .open .dropdown-menu {
  position: static;
  float: none;
  width: auto;
  margin-top: 0;
  background-color: transparent;
  border: 0;
  box-shadow: none;
}
.aside-fixed .navbar-inverse .navbar-nav .open .dropdown-menu>li>a {
  color: #9d9d9d;
  padding: 5px 15px 5px 50px;
}
.aside-fixed .navbar-inverse .navbar-nav .open .dropdown-menu>li>a:hover, .aside-fixed .navbar-inverse .navbar-nav .open .dropdown-menu>li>a:focus {
  color: #fff;
  background-color: transparent;
}
/* ASIDE MENU END */

/* FOOTER */
footer.navbar-inverse {
