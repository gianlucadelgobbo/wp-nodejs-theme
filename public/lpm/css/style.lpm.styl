/* LISTS */
ul.lists{margin: 0; padding:0;list-style: none;}
.itemListElement {
  position: relative;
  margin-bottom: 20px;
  overflow: hidden;
}
.itemListElement .media{
  margin-bottom: 10px;
}
.caption {
  position: absolute;
  pointer-events: none;
  color: #FFF;
  left: 0;
  top: 0;
  bottom: 0;
  right: 0;
  bottom: 0px;
  overflow: hidden;
}
.caption-time {
  background: rgba(255, 255, 255, .5);
  color: #000;
  padding: 5px 10px;
  display: inline-block;
  font-size: 10px;
}
.caption-body {
  position: absolute;
  bottom: 0;
  padding: 10px;
  text-shadow: 0 2px 4px black;
}
.caption-type {
  position: absolute;
  -ms-transform: rotate(-90deg);
  -webkit-transform: rotate(-90deg);
  right: -200px;
  top: 0;
  transform: rotate(90deg);
  transform-origin: top left;
  width: 200px;
}
.caption-title {
  font-size: 21px;
  margin-top: 0;
}
.caption-location, .caption-location b {
  font-weight: normal;
}
.caption-location-block {
  font-size: 10px;
  display: block;
}
.caption-type-cnt {
  position: relative;
  display: inline-block;
  background: rgba(255, 255, 255, .5);
  color: #000;
  padding: 5px 10px;
  font-size: 15px;
  text-transform: uppercase;
}
.caption-soldout {
  position: absolute;
  -ms-transform: rotate(-45deg);
  -webkit-transform: rotate(-45deg);
  right: -82px;
  top: -19px;
  transform: rotate(45deg);
  transform-origin: top left;
  width: 200px;
  text-align: center;
  background-color: #FF0000;
  font-weight: bold;
  z-index: 1;
  font-size: 20px;
}
.lists .media-heading, #listItems .media-heading {
  font-size: 15px;
}
.row.partners .col-md-5, .row.gallery .col-md-5 {
  margin-right: -15px;
}
.row.partners {
  margin-bottom: 15px;
}
.row.partners .stit {
  font-size: 11px;
}
#listItems {
  padding: 0;
}
/* END LISTS */

