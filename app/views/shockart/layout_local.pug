doctype html
html(lang="en")
  head
    title=page_data.title
    meta(charset='UTF-8')
    meta(name='viewport', content='width=device-width, initial-scale=1.0')
    //- //console.log(config.meta)
    link(rel='stylesheet', href='/' + config.prefix + '/css/combo.min.css', type='text/css', media='all')
    //link(rel='stylesheet', href='/_common/css/fontello.css', type='text/css', media='all')
    if include_gallery
      link(rel='stylesheet', href='/_common/css/socialGalleryPluginLite.css', type='text/css', media='all')
    //link(rel='stylesheet', href='/_common/css/cookielawinfo.css', type='text/css', media='all')
      link(rel='stylesheet', href='/css/bootstrap.min.css', type='text/css')
      link(rel='stylesheet', href='/_common/css/bootstrapXL.css', type='text/css', media='all')
      link(rel='stylesheet', href='/_common/css/style.css', type='text/css', media='all')
      link(rel='stylesheet', href='/css/style.css', type='text/css', media='all')
  body
    .container
      header#myHeader.hidden-xs
        .row
          .col-sm-8
            h2
              a(href='/', title=config.project_name)
                img.img-responsive(src='/shockart/images/shockart_logo_full.svg', alt=config.project_name)
          .col-sm-4
            h3.text-right=config.meta.headline["en"]
      #result
        block content
      //include includes/footer
    include ../_common/modal-full
    include ../_common/modal-search
    include includes/modal-resize

    script(type='text/javascript', src='/_common/js/combo.min.js')
    if form
      script(type='text/javascript', src='/_common/js/validator.min.js')
    //script(type='text/javascript', src='/_common/js/jquery.min.js')
      script(type='text/javascript', src='/_common/js/bootstrap.min.js')
      script(type='text/javascript', src='/_common/js/jquery.isotope.min.js')
      script(type='text/javascript', src='/_common/js/imagesloaded.pkgd.min.js')
      script(type='text/javascript', src='/_common/js/cookielawinfo.min.js')
    include ../_common/inc_cookielawinfo

    if include_gallery
      #fb-root
      script.
        (function (d, s, id) {
          var js, fjs = d.getElementsByTagName(s)[0];
          if (d.getElementById(id)) return;
          js = d.createElement(s);
          js.id = id;
          js.src = "//connect.facebook.net/en_US/sdk.js#xfbml=1&version=v2.8&appId=#{config.meta.fb_appId}";
          fjs.parentNode.insertBefore(js, fjs);
        }(document, 'script', 'facebook-jssdk'));
      script(type="text/javascript").
        var sgp_config = {
          "sgp_selT": "2",
          "sgp_sel": ".thumbnail",
          "sgp_bT": "#{config.project_name}",
          "sgp_bL": "#{config.meta.headline[sessions.current_lang]}",
          "sgp_bU": "#{global.config.domain}",
          "sgp_bg": "000000",
          "sgp_bgo": "0.8",
          "sgp_bb": "1",
          "sgp_hb": "1",
          "sgp_hbt": "1",
          "sgp_ch": "",
          "sgp_hbi": '/#{config.prefix}/images/social-gallery-icon-50x50.png',
          "sgp_desc": "1",
          "sgp_fb": "1",
          "sgp_fbf": "1",
          "sgp_fbc": "1",
          "sgp_nav": "1",
          "sgp_hon": "",
          "sgp_usf": "1.2",
          "sgp_mb": "10",
          "sgp_theme": "classic",
          /*"sgp_theme_root": "http://flyer.it/wp-content/plugins/flyer-social-gallery/themes/",*/
          "sgp_iLoadR": '/_common/images/social-gallery-loading.gif',
          "sgp_iR": '/_common/images/social-gallery-r.png',
          "sgp_iL": '/_common/images/social-gallery-l.png'
        };
        var sgp_ie = false;var sgp_ie7 = false;
      script(type='text/javascript', src='/_common/js/jquery-migrate.min.js')
      script(type='text/javascript', src='/_common/js/socialGalleryPluginLite.min.js')

    //script(type='text/javascript', src='/_common/js/script.min.js')
    script(type='text/javascript', src='/' + config.prefix + '/js/script.min.js')
