doctype xml
urlset(xmlns='http://www.sitemaps.org/schemas/sitemap/0.9')
  each result in results.tags
    url
      loc=config.domain+"/"+config.sez[results.name].baseurl +"/tags/"+result.slug+"/"
      if (config.locales.length > 1)
        each locale in config.locales
          if locale == config.default_lang
            xhtml:link(rel="alternate", hreflang=locale, href=config.domain + "/" + config.sez[results.name].baseurl +"/tags/"+result.slug+"/")
          else
            xhtml:link(rel="alternate", hreflang=locale, href=config.domain + "/" + locale + "/" + config.sez[results.name].baseurl +"/tags/"+result.slug+"/")

      lastmod=config.sitemap.lastmod
      changefreq monthly
      priority 1.0
