.boxItem(itemprop="itemListElement", itemscope, itemtype="http://schema.org/ListItem")
  meta(itemprop="position", content=position+1)
  div(itemprop="item", itemscope, itemtype='http://schema.org/Person')
    .row
      .col-sm-4(itemprop="image", itemscope, itemtype='https://schema.org/ImageObject')
        a(href=lang_preurl+'/'+sez.baseurls[result.role]+'/'+result.user_login+'/', rel='bookmark', title='Logo: '+result.display_name)
          img.img-responsive.svg-image(alt='Logo: '+result.display_name, itemprop="url", src=result.img)
          meta(itemprop="width" content="480")
          meta(itemprop="height" content="270")
      .col-sm-8
        h5.media-heading
          a(itemprop='url', href=lang_preurl+'/'+sez.baseurls[result.role]+'/'+result.user_login+'/')
            span(itemprop='name')=result.display_name
        p(itemprop='jobTitle')
          i=result.role_label
        if result.auth_contents
          p
            each auth_content,key in result.auth_contents
              if auth_content
                !=config.posttypes_labels[key]+": "+auth_content
                br
    hr
