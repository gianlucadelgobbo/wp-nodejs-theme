.boxItem(itemscope, itemtype='http://schema.org/itemListElement')
  .LabHome(itemscope, itemtype='http://schema.org/CreativeWork')
    .row
      .col-lg-3
        .thumb-cnt
          //.catlabel=result.type
          a.ajaxloader(itemprop='url', href='/portfolio/lab/'+result.slug+"/", rel='bookmark', title!=result.post_title)
            img.img-responsive(src=result.featured.thumbnail, alt!=global.__('Image for')+': '+result.post_title, itemprop='image', itemscope, itemtype='https://schema.org/ImageObject')
      .col-lg-9
        .box-list
          .box-list-cnt
            h3(itemprop='name')
              a.ajaxloader(itemprop='url', href='/portfolio/lab/'+result.slug+"/", rel='bookmark', title=result.post_title)!=result.post_title
            if result.data_month
              .text-muted
                .dates
                  .date=result.data_month
            .text-muted.small.excerpt
              if result.excerpt && result.excerpt.rendered
                div(itemprop='headline')
                  p!=result.excerpt.rendered
